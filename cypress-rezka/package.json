{
    "name": "cypress-rezka",
    "version": "0.1.0",
    "author": "",
    "license": "MIT",
    "scripts": {
        "tsc": "tsc -p tsconfig.json",
        "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts,tsx}\" &&  eslint \"cypress/**/*.{js,ts,tsx}\"",
        "lint-fix": "tsc --noEmit && eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix && eslint \"cypress/**/*.{js,ts,tsx}\" --quiet --fix",
        "prettier": "prettier --config ./.prettierrc.js --check \"src/**/*{.ts,.tsx,*.js,*.json.*.babelrc}\" --check \"cypress/**/*{.ts,.tsx,*.js,*.json.*.babelrc}\" ",
        "prettier-fix": "prettier --config ./.prettierrc.js --check \"src/**/*{.ts,.tsx,*.js,*.json.*.babelrc}\" --check \"cypress/**/*{.ts,.tsx,*.js,*.json.*.babelrc}\" --write",
        "test": "jest --config=jest.config.js",
        "validate": "yarn lint && yarn test && yarn tsc",
        "cy": " cypress open watchForFileChanges=false --config-file cypress.json ",
        "cy-silent-rezka": "npx cypress run --config watchForFileChanges=false --browser chrome --headless --spec \"cypress/integration/temp/**/*\"",
        "cy-setup": "npx cypress run --config watchForFileChanges=false --browser chrome --headless --spec \"cypress/integration/setup/**/*\"",
        "t": "env-cmd -f .env.local cross-env NODE_ENV=development cross-env REACT_APP_NODE_ENV=cypress npx babel-node-ts --config-file ./scripts/custom-babel-config.js scripts/generate-translations.ts",
        "t:all": "yarn build-i18n && yarn t",
        "analyze": "source-map-explorer 'build/static/*.js'"
    },
    "dependencies": {
        "add": "^2.0.6",
        "axios": "^1.4.0",
        "babel-loader": "8.1.0",
        "babel-plugin-import": "^1.13.3"
    },
    "devDependencies": {
        "@babel/node": "^7.6.1",
        "@babel/preset-typescript": "^7.18.6",
        "@types/eslint": "^7.28.0",
        "@types/jest": "^26.0.24",
        "@typescript-eslint/eslint-plugin": "^4.28.4",
        "@typescript-eslint/experimental-utils": "^4.28.4",
        "@typescript-eslint/parser": "^4.28.4",
        "babel-loader": "8.1.0",
        "babel-node-ts": "^1.0.3",
        "babel-plugin-import": "^1.13.3",
        "babel-plugin-istanbul": "^6.1.1",
        "babel-plugin-macros": "^3.1.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-transform-inline-environment-variables": "^0.4.3",
        "babel-plugin-twin": "^1.0.2",
        "babel-preset-react-app": "^10.0.0",
        "concurrently": "^6.2.0",
        "cross-env": "^7.0.3",
        "css-loader": "3.4.2",
        "cypress": "8.1.0",
        "cypress-plugin-stripe-elements": "^1.0.2",
        "env-cmd": "^10.1.0",
        "eslint": "7.32.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-cypress": "^2.11.3",
        "eslint-plugin-hooks-sort": "^0.1.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-no-only-tests": "^2.6.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-unicorn": "^35.0.0",
        "jest-less-loader": "^0.1.1",
        "js-yaml": "^4.1.0",
        "less-loader": "6.0.0",
        "lint-staged": "^11.1.1",
        "mini-css-extract-plugin": "^0.9.0",
        "nyc": "^15.1.0",
        "prettier": "2.3.2",
        "prettier-plugin-organize-imports": "^1.1.1",
        "react-app-rewire-alias": "^1.1.7",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-test-renderer": "16.13.1",
        "source-map-explorer": "^2.5.3",
        "stylelint": "^13.12.0",
        "stylelint-order": "^4.1.0",
        "typescript": "4.3.2",
        "url-loader": "^4.1.1",
        "yarn": "^1.22.11"
    },
    "lint-staged": {
        "**/*.{ts,tsx,js,json}": [
            "npm run lint-fix",
            "npm run prettier-fix",
            "git add"
        ]
    },
    "babelMacros": {
        "twin": {
            "preset": "emotion"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
